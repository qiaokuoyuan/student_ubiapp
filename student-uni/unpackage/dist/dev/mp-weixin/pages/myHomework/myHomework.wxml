<view><uni-nav-bar vue-id="1" statusBar="{{true}}" title="我的作业" bind:__l="__l"></uni-nav-bar><view class="uni-flex uni-row"><block wx:if="{{false}}"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="{{[[(tab!='all')?'unselect':''],[(tab=='all')?'select':'']]}}" bindtap="__e"><text style="line-height:100rpx;font-size:larger;">全部(debug)</text></view></block><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="{{[[(tab!='unfinished')?'unselect':''],[(tab=='unfinished')?'select':'']]}}" bindtap="__e"><text style="line-height:100rpx;font-size:larger;">未完成</text></view><view data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" class="{{[[(tab!='finished')?'unselect':''],[(tab=='finished')?'select':'']]}}" bindtap="__e"><text style="line-height:100rpx;font-size:larger;">已完成</text></view></view><view><block wx:for="{{list_homework}}" wx:for-item="h" wx:for-index="hi" wx:key="hi"><view><block wx:if="{{h.Extension=='已作答'&&tab=='finished'}}"><view style="height:100rpx;border-bottom:solid 1rpx #EEE8AA;padding:0 30rpx;"><text data-event-opts="{{[['tap',[['doHomework',['$0'],[[['list_homework','',hi]]]]]]]}}" style="line-height:100rpx;" bindtap="__e">{{h.Content}}</text></view></block><block wx:if="{{h.Extension=='未作答'&&tab=='unfinished'}}"><view style="height:100rpx;border-bottom:solid 1rpx #EEE8AA;padding:0 30rpx;"><text data-event-opts="{{[['tap',[['doHomework',['$0'],[[['list_homework','',hi]]]]]]]}}" style="line-height:100rpx;" bindtap="__e">{{h.Content}}</text></view></block><block wx:if="{{tab=='all'}}"><view style="height:100rpx;border-bottom:solid 1rpx #EEE8AA;padding:0 30rpx;"><text data-event-opts="{{[['tap',[['doHomework',['$0'],[[['list_homework','',hi]]]]]]]}}" style="line-height:100rpx;" bindtap="__e">{{h.Content}}</text></view></block></view></block></view></view>