<view><uni-nav-bar vue-id="1" statusBar="{{true}}" title="我的作品" bind:__l="__l"></uni-nav-bar><view hidden="{{!(!show_editor)}}" style="margin:30rpx;"><block wx:for="{{list_myArtifact}}" wx:for-item="a" wx:for-index="ai" wx:key="ai"><view data-event-opts="{{[['tap',[['toArticatDetail',['$0'],[[['list_myArtifact','',ai]]]]]]]}}" class="uni-flex uni-row" style="margin:10rpx;" bindtap="__e"><view><block wx:if="{{a.Cover}}"><image style="width:200rpx;height:150rpx;" src="{{a.Cover}}" mode="scaleToFill"></image></block><block wx:else><image style="width:200rpx;height:150rpx;" src="../../../static/video.png" mode="scaleToFill"></image></block></view><view class="uni-flex uni-column" style="margin-left:30rpx;"><view style="height:70rpx;"><text style="line-height:70rpx;">{{a.StudentWorkName}}</text></view><view style="height:70rpx;"><block wx:if="{{a.ReviewFlag==1}}"><text class="kcstatA" style="line-height:70rpx;color:#FFDAB9;">【已通过】</text></block><block wx:if="{{a.ReviewFlag==2}}"><text class="kcstatC" style="line-height:70rpx;color:#BCEE68;">【未通过】</text></block><block wx:if="{{a.ReviewFlag==99}}"><text class="kcstatD" style="line-height:70rpx;color:#FFEC8B;">【草稿】</text></block><block wx:if="{{a.ReviewFlag==0}}"><text class="kcstatE" style="line-height:70rpx;color:#FFC1C1;">【待审核】</text></block></view></view></view></block></view><view hidden="{{!(!show_editor)}}"><uni-fab class="vue-ref" vue-id="2" pattern="{{pattern}}" content="{{fab_content}}" horizontal="right" vertical="bottom" direction="horizontal" data-ref="fab" data-event-opts="{{[['^trigger',[['trigger']]]]}}" bind:trigger="__e" bind:__l="__l"></uni-fab></view><view hidden="{{!(show_editor)}}"><myeditor style="width:100%;height:100%;overflow:scroll;" vue-id="3" bind:__l="__l"></myeditor><button style="margin:30rpx;" type="primary" data-event-opts="{{[['tap',[['addArtifact',['confirm']]]]]}}" bindtap="__e">完成编创</button><button style="margin:30rpx;" type="primary" data-event-opts="{{[['tap',[['addArtifact',['close']]]]]}}" bindtap="__e">取消</button></view></view>