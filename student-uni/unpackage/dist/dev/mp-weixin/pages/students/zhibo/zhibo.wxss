
.select {
	width: 100%;
	height: 100rpx;
	background-color: #00ced1;
	border-bottom: solid 5rpx #1e90ff;
	text-align: center;
	-webkit-transition-duration: 1s;
	        transition-duration: 1s;
}
.unselect {
	width: 100%;
	height: 100rpx;
	background-color: #faebd7;
	border-bottom: solid 5rpx #faebd7;
	text-align: center;
	-webkit-transition-duration: 1s;
	        transition-duration: 1s;
}

