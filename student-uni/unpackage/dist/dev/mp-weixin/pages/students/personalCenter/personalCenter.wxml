<view><uni-nav-bar vue-id="1" statusBar="{{true}}" leftIcon="arrowleft" title="个人中心" bind:__l="__l"></uni-nav-bar><view hidden="{{!(isLogin)}}"><view class="uni-flex" style="margin-top:100rpx;margin-bottom:10rpx;-webkit-justify-content:center;justify-content:center;"><image style="height:200rpx;width:200rpx;" src="{{userLogo}}" mode="scaleToFill"></image></view><view class="uni-flex" style="margin-top:10rpx;margin-bottom:3rpx;-webkit-justify-content:center;justify-content:center;"><view><text>{{UnitName}}</text></view></view><view class="uni-flex" style="margin-top:10rpx;margin-bottom:3rpx;-webkit-justify-content:center;justify-content:center;"><view><text>{{RealName}}</text></view></view><uni-list vue-id="2" bind:__l="__l" vue-slots="{{['default']}}"><uni-list-item vue-id="{{('3')+','+('2')}}" title="我的离线课程" thumb="https://img-cdn-qiniu.dcloud.net.cn/new-page/uni.png" data-event-opts="{{[['^click',[['toMyOffline']]]]}}" bind:click="__e" bind:__l="__l"></uni-list-item><uni-list-item vue-id="{{('4')+','+('2')}}" title="修改密码" thumb="https://img-cdn-qiniu.dcloud.net.cn/new-page/uni.png" bind:__l="__l"></uni-list-item><uni-list-item vue-id="{{('5')+','+('2')}}" title="清除缓存" badge-text="{{badge}}" show-badge="{{true}}" thumb="https://img-cdn-qiniu.dcloud.net.cn/new-page/uni.png" data-event-opts="{{[['^click',[['clearOfflineFiles']]]]}}" bind:click="__e" bind:__l="__l"></uni-list-item><uni-list-item vue-id="{{('6')+','+('2')}}" title="版本更新" thumb="https://img-cdn-qiniu.dcloud.net.cn/new-page/uni.png" bind:__l="__l"></uni-list-item></uni-list><button style="margin:40rpx;" type="primary" data-event-opts="{{[['tap',[['exit']]]]}}" bindtap="__e">退出登陆</button></view><block wx:if="{{!isLogin}}"><view style="margin:40rpx;"><image style="height:300rpx;width:100%;" src="../../../static/default_course_logo.jpg" mode="scaleToFill"></image><input style="border:solid 3rpx #00E5EE;border-radius:10rpx;font-size:x-large;" type="text" placeholder="账号" data-event-opts="{{[['input',[['__set