<view><uni-nav-bar vue-id="1" leftIcon="arrowleft" data-event-opts="{{[['^clickLeft',[['back']]]]}}" bind:clickLeft="__e" bind:__l="__l"></uni-nav-bar><swiper class="swiper" circular indicator-dots="{{true}}" autoplay="{{true}}" interval="{{4000}}"><swiper-item><view class="swiper-item uni-bg-red">A</view></swiper-item><swiper-item><view class="swiper-item uni-bg-green">B</view></swiper-item><swiper-item><view class="swiper-item uni-bg-blue">C</view></swiper-item></swiper><view><uni-section vue-id="2" title="{{courseTitle}}" type="line" bind:__l="__l"></uni-section><view style="margin:30rpx;"><text>{{courseDesc}}</text></view></view><view><uni-section vue-id="3" title="教师团队" type="line" bind:__l="__l"></uni-section><view><block wx:for="{{courseTeachers}}" wx:for-item="t" wx:for-index="ti" wx:key="ti"><view class="uni-flex uni-row" style="margin:30rpx;"><block wx:if="{{t.Logo}}"><image style="width:120rpx;height:120rpx;" src="{{t.Logo}}" mode="scaleToFill"></image></block><block wx:else><image style="width:120rpx;height:120rpx;" src="../../../static/logo.png" mode="scaleToFill"></image></block><view class="uni-column" style="margin-top:20rpx;margin-left:30rpx;"><view><text style="font-size:larger;">{{t.RealName}}</text></view><view><text>{{t.Department}}</text></view></view></view></block></view></view><button style="margin:40rpx;" type="primary" data-event-opts="{{[['tap',[['toLearnCourse']]]]}}" bindtap="__e">开始学习</button></view>