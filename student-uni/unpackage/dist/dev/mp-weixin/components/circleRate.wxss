
.content {
	width: 100%;
	height: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-flex-flow: wrap;
	        flex-flow: wrap;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	border-radius: 50%;
	text-align: center;
	height: 400rpx;
	padding-top: 50px;
}
.circleprogress {
	width: 100%;
	height: 200px;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.progresstext {
	position: absolute;
	font-size: 40px;
	width: 200px;
	height: 200px;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	z-index: 10;
	color: #008000;
}
.wrapper {
	width: 100px;
	height: 200px;
	overflow: hidden;
}
.leftprogress,
.rightprogress {
	width: 160px;
	height: 160px;
	border: 20px solid green;
	border-bottom: 20px solid #7ef22e;
	border-radius: 50%;
}
.leftprogress {
	border-right: 20px solid #7ef22e;
}
.rightprogress {
	border-left: 20px solid #7ef22e;
	margin-left: -100px;
}
.section {
	width: 80%;
	margin-top: 50px;
}

